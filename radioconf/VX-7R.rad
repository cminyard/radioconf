list Memories 450(0x1322)
#	Name			Address			type
	"Enable"		(0x1202,1,1,4)		CheckBox
	"Mask"			(0x1202,0,1,4)		RCheckBox

	"Label"			(6*,0,64,176)		YaesuLCString(8)

	"Rx\nFreq"		(2*,0,24,176)		BCDFreq2 "tie(basefreq)=Tx\nFreq"
	"Offset\nDirect"	(1*,4,2,176)		OffsetDir_e "tie(offset)=Tx\nFreq" "tie(enableoff)=Offset\nFreq" "tie(enableoff)=Offset\nFTab"
	"Tx\nFreq"		(15*,0,24,176)		BCDFreq2
	"Offset\nFreq"		(15*,0,24,176)		BCDFixedFreq(2,3,50,MHz,w=8) "tie=Offset\nFTab" "tie(freqoff)=Tx\nFreq" cutpaste=False
	"Offset\nFTab"		(15*,0,24,176)		OffsetFreq_e "tie=Offset\nFreq" "tie(freqoff)=Tx\nFreq" cutpaste=False

	"Tone\nMode"		(20*,0,3,176)		ToneMode_e tone=both
	"CTCSS\nTone"		(18*,0,8,176)		CTCSS_e
	"DCS\nCode"		(19*,0,8,176)		DCSCode_e
	"Xmit\nPower"		(1*,6,2,176)		XmitPower_e
# Called "Half Deviation" in the VX-7R
	"Tx\nNarrow"		(0*,5,1,176)		CheckBox
	"Step"			(1*,0,3,176)		Step_e
	"Mode"			(5*,0,2,176)		ReceiveMode_e
# Called "CPU Shift" in the VX-7R
	"Clock\nShift"		(0*,4,1,176)		CheckBox

	"Scan\nMode"		(0x1202,2,2,4)		ScanMode_e

	"Char\nSet"		(0*,0,1)		Empty
	"C1"			(21*,0,1,176)		CheckBox
	"C2"			(21*,1,1,176)		CheckBox
	"C3"			(21*,2,1,176)		CheckBox
	"C4"			(21*,3,1,176)		CheckBox
	"C5"			(21*,4,1,176)		CheckBox
	"C6"			(21*,5,1,176)		CheckBox
	"C7"			(21*,6,1,176)		CheckBox
	"C8"			(21*,7,1,176)		CheckBox
endlist

